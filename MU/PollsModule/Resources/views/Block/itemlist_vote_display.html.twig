{# Purpose of this template: Display votes within an external context #}
{% for vote in items %}
    <h3>{{ vote|mupollsmodule_formattedTitle }}</h3>
    <p><a href="{{ path('mupollsmodule_vote_display', {'id': vote.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
