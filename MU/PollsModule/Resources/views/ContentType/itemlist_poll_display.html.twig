{# Purpose of this template: Display polls within an external context #}
{% for poll in items %}
    <h3>{{ poll|mupollsmodule_formattedTitle }}</h3>
    <p><a href="{{ path('mupollsmodule_poll_display', {'id': poll.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
