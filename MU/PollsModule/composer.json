{
    "name": "mu/polls-module",
    "version": "1.2.0",
    "description": "MUPollsModule module generated by ModuleStudio 1.0.2.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\PollsModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.5.1 <3.0",
            "class": "MU\\PollsModule\\MUPollsModule",
            "displayname": "Polls",
            "url": "polls",
            "capabilities": {
                "user": {"route": "mupollsmodule_poll_index"},
                "admin": {"route": "mupollsmodule_poll_adminindex"}
            },
            "securityschema": {
                "MUPollsModule::": "::",
                "MUPollsModule:ItemListBlock:": "Block title::",
                "MUPollsModule:Option:": "Option ID::",
                "MUPollsModule:Poll:": "Poll ID::",
                "MUPollsModule:Vote:": "Vote ID::",
                "MUPollsModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "platform": {
            "php": "5.5.9"
        }
    }
}
